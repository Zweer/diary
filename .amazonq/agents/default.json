{
  "name": "default-agent",
  "version": "1.0.0",
  "description": "Default agent configuration",
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "disabled": false,
      "timeout": 60000,
      "args": [
        "-y",
        "@executeautomation/playwright-mcp-server"
      ]
    }
  },
  "tools": [
    "fs_read",
    "execute_bash",
    "fs_write",
    "report_issue",
    "use_aws",
    "@playwright"
  ],
  "allowedTools": [
    "fs_read",
    "report_issue",
    "use_aws",
    "execute_bash",
    "fs_write",
    "@playwright/playwright_navigate",
    "@playwright/playwright_screenshot",
    "@playwright/playwright_close"
  ],
  "toolsSettings": {
    "use_aws": {
      "preset": "readOnly"
    },
    "execute_bash": {
      "preset": "readOnly"
    }
  },
  "includedFiles": [
    "AmazonQ.md",
    "README.md",
    ".amazonq/rules/**/*.md"
  ],
  "resources": []
}